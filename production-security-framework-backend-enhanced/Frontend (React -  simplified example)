(This would require a much more substantial React application with routing, styling, and component structure.  This is a highly simplified example)
//src/App.js
import React from 'react';
import axios from 'axios';


function App() {
  const [data, setData] = React.useState(null);

  React.useEffect(() => {
    const fetchData = async () => {
      try {
        const response = await axios.get('/products'); // Replace with your API endpoint
        setData(response.data);
      } catch (error) {
        console.error("Error fetching data:", error);
      }
    };
    fetchData();
  }, []);


  return (
    <div>
      <h1>Products</h1>
      {data && data.map(item => <p key={item.id}>{item.name} - ${item.price}</p>)}
    </div>
  );
}

export default App;